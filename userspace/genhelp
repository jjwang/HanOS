#!/bin/sh

cat <<EOF >_help.c

//
// Auto-generated by genhelp.
// DO NOT EDIT THIS FILE
//

#include <stdint.h>
#include <stddef.h>

#include <userspace/help.h>

const command_help_t _shell_helptab[] = {
EOF

grep \<help\> *.c | sort -t: -k2 | sed 's/^\w*.c://g' >> _help.c

cat <<EOF >> _help.c
    {"", ""}
};
EOF

