#!/bin/sh

echo "/*\n    Auto-generated by gensym." > _symbols.gen
echo "    DO NOT EDIT THIS FILE\n*/\n\n#include \"symbols.h\"\n" >> _symbols.gen
echo "const symbol_t _kernel_symtab[] = {" >> _symbols.gen
nm $1 | grep " T " | awk '{ print "    { .addr = 0x"$1", .name = \""$3"\" }," }' | sort >> _symbols.gen
echo "    { .addr = UINT64_MAX, .name = \"\" }\n};" >> _symbols.gen
